  <org.eclipse.epf.uma:DescriptorDescription xmi:id="-FPEjXpJcBZLLdyqWQsg4DQ" name="test_solution,_jevfsETCEei8NvIaNnYqRg" guid="-FPEjXpJcBZLLdyqWQsg4DQ"/>
  <org.eclipse.epf.uma:DescriptorDescription xmi:id="-RXgMuhlAAU0eCu9ztFdt4g" name="software_developer,_jevfsUTCEei8NvIaNnYqRg" guid="-RXgMuhlAAU0eCu9ztFdt4g"/>
